/ {
	named-gpios {
		/* Lotus-specific GPIO customizations */
	};

	named-temp-sensors {
		compatible = "cros-ec,temp-sensors";
		local-f75303 {
			temp_host_warn = <90>;
			temp_host_high = <90>;
			temp_host_halt = <127>;
			temp_host_release_warn = <40>;
			temp_host_release_high = <40>;
			temp_fan_off = <40>;
			temp_fan_max = <52>;
			power-good-pin = <&gpio_slp_s5_l>;
			sensor = <&local_f75303>;
		};
		ddr-f75303 {
			temp_host_warn = <90>;
			temp_host_high = <90>;
			temp_host_halt = <127>;
			temp_host_release_warn = <40>;
			temp_host_release_high = <40>;
			temp_fan_off = <40>;
			temp_fan_max = <52>;
			power-good-pin = <&gpio_slp_s5_l>;
			sensor = <&ddr_f75303>;
		};
		cpu-f75303 {
			temp_host_warn = <90>;
			temp_host_high = <90>;
			temp_host_halt = <127>;
			temp_host_release_warn = <40>;
			temp_host_release_high = <40>;
			temp_fan_off = <40>;
			temp_fan_max = <60>;
			power-good-pin = <&gpio_slp_s5_l>;
			sensor = <&cpu_f75303>;
		};
		amd-apu {
			temp_host_warn = <108>;
			temp_host_high = <108>;
			temp_host_halt = <127>;
			temp_host_release_warn = <60>;
			temp_host_release_high = <60>;
			temp_fan_off = <108>;
			temp_fan_max = <108>;
			power-good-pin = <&gpio_slp_s3_l>;
			sensor = <&temp_cpu>;
		};
		gpu-amb-f75303 {
			temp_host_warn = <100>;
			temp_host_high = <100>;
			temp_host_halt = <127>;
			temp_host_release_warn = <40>;
			temp_host_release_high = <40>;
			temp_fan_off = <40>;
			temp_fan_max = <70>;
			power-good-pin = <&gpio_slp_s5_l>;
			sensor = <&gpu_amb_f75303>;
		};
		gpu-vr-f75303 {
			temp_host_warn = <100>;
			temp_host_high = <100>;
			temp_host_halt = <127>;
			temp_host_release_warn = <40>;
			temp_host_release_high = <40>;
			temp_fan_off = <40>;
			temp_fan_max = <70>;
			power-good-pin = <&gpio_slp_s5_l>;
			sensor = <&gpu_vr_f75303>;
		};
		gpu-vram-f75303 {
			temp_host_warn = <100>;
			temp_host_high = <100>;
			temp_host_halt = <127>;
			temp_host_release_warn = <40>;
			temp_host_release_high = <40>;
			temp_fan_off = <40>;
			temp_fan_max = <70>;
			power-good-pin = <&gpio_slp_s5_l>;
			sensor = <&gpu_vram_f75303>;
		};
		gpu-amdr23m {
			temp_host_warn = <108>;
			temp_host_high = <108>;
			temp_host_halt = <127>;
			temp_host_release_warn = <60>;
			temp_host_release_high = <60>;
			temp_fan_off = <108>;
			temp_fan_max = <108>;
			power-good-pin = <&gpio_slp_s5_l>;
			sensor = <&gpu_amdr23m>;
		};
	};

	typec-port {
		#address-cells = <1>;
		#size-cells = <0>;

		/*
		 * If the CHARGER_SINGLE_CHIP config is enabled, only
		 * one charger chip must be defined.
		 *
		 * In the Zephyer OS, the CONFIG_USB_PD_PORT_MAX_COUNT will check
		 * the device tree compatible "named-usbc-port"
		 */
		usbc_port0: port0@0 {
			compatible = "named-usbc-port";
			reg = <0>;
			chg = <&charger>;
		};
		usbc_port1: port1@1 {
			compatible = "named-usbc-port";
			reg = <1>;
		};
		usbc_port2: port2@2 {
			compatible = "named-usbc-port";
			reg = <2>;
		};
		usbc_port3: port3@3 {
			compatible = "named-usbc-port";
			reg = <3>;
		};
	};
};

&cros_kb_raw {
	status = "okay";
	pinctrl-0 = <>;
	pinctrl-names = "default";
};

&bbram_regions {
	charge_limit_max {
		offset = <0x44>;
		size = <0x01>;
	};
	fp_led_level {
		offset = <0x45>;
		size = <0x01>;
	};
};
